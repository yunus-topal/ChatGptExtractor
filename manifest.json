{
  "manifest_version": 3,
  "name": "ChatGPT Dialogue Extractor",
  "version": "1.1",
  "description": "Extracts ChatGPT dialogue as JSON.",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
